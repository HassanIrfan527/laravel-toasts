{
    "name": "hassanirfan527/laravel-toasts",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.7",
        "livewire/flux": "^2.10"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "HassanIrfan527\\LaravelToasts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HassanIrfan527\\LaravelToasts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HassanIrfan527\\LaravelToasts\\LaravelToastsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "HassanIrfan527",
            "email": "harryed525@gmail.com"
        }
    ]
}
